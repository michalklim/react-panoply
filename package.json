{
  "name": "react-panoply",
  "version": "0.0.0",
  "description": "Robust, performant, customizable, SSR friendly carousel",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "check-types": "tsc",
    "dev": "start-storybook --port 3000",
    "build": "webpack --env.mode production && tsc -b tsconfig.production.json",
    "debug": "node --inspect-brk ./node_modules/webpack/bin/webpack.js",
    "dev:debug": "npm run debug -- --env.mode development"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michalklim/react-panoply.git"
  },
  "keywords": [
    "react",
    "carousel",
    "slider",
    "SSR"
  ],
  "author": "Michal KLim",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/michalklim/react-panoply/issues"
  },
  "homepage": "https://github.com/michalklim/react-panoply#readme",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "peerDependencies": {
    "react": ">= 16.8.0",
    "react-dom": ">= 16.8.0"
  },
  "devDependencies": {
    "@babel/core": "7.8.3",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "7.8.3",
    "@babel/preset-env": "7.8.3",
    "@babel/preset-react": "7.8.3",
    "@babel/preset-typescript": "7.8.3",
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@storybook/react": "5.3.6",
    "@types/classnames": "^2.2.9",
    "@types/react": "16.9.17",
    "@types/react-dom": "16.9.4",
    "@types/storybook__react": "5.2.1",
    "@typescript-eslint/eslint-plugin": "2.16.0",
    "@typescript-eslint/parser": "2.16.0",
    "babel-loader": "8.0.6",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.9.0",
    "eslint-loader": "3.0.3",
    "eslint-plugin-react": "7.18.0",
    "fork-ts-checker-webpack-plugin": "4.0.1",
    "html-webpack-plugin": "3.2.0",
    "husky": "4.0.10",
    "prettier": "1.19.1",
    "pretty-quick": "2.0.1",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "source-map-loader": "0.2.4",
    "tsconfig-paths-webpack-plugin": "3.2.0",
    "typescript": "3.7.5",
    "webpack": "4.41.5",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.10.1",
    "webpack-merge": "4.2.2"
  },
  "dependencies": {
    "classnames": "^2.2.6"
  }
}
